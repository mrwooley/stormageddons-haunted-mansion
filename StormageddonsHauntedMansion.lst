				; ---------------------------------------------------------------------------------
				; Name: printTextFile
				;
				; Prints the splash page.
				;
				; Preconditions: none.
				;
				; Postconditions: Value stored in EAX.
				;
				; Receives:
				;	[EBP+8] = offset of file
				;
				; Returns: none.
				; ---------------------------------------------------------------------------------
 00000062			printTextFile PROC
 00000062  55				push EBP
 00000063  8B EC			mov EBP, ESP
 00000065  50				push EAX
 00000066  51				push ECX
 00000067  52				push EDX

					; Open file and create handle
 00000068  8B 55 08			mov EDX, [EBP+8]
 0000006B  E8 00000000 E		call OpenInputFile
 00000070  50				push EAX

					; Read file
 00000071  BA 000000CC R		mov EDX, OFFSET txtArray
 00000076  B9 00000FA0			mov ECX, ARRAY_SIZE
 0000007B  E8 00000000 E		call ReadFromFile

					; Close file
 00000080  58				pop EAX
 00000081  E8 00000000 E		call CloseFile

					; Print text
 00000086  BA 000000CC R		mov EDX, OFFSET txtArray
 0000008B  E8 00000000 E		call WriteString
 00000090  E8 00000000 E		call CrLf
 00000095  E8 00000000 E		call CrLf
					
					; Clear array
 0000009A  68 00000FA0			push ARRAY_SIZE
 0000009F  68 000000CC R		push OFFSET txtArray
 000000A4  E8 000000EA			call clearArray

 000000A9  5A				pop EDX
 000000AA  59				pop ECX
 000000AB  58				pop EAX
 000000AC  5D				pop EBP
 000000AD  C2 0004			ret 4
 000000B0			printTextFile ENDP

				; ---------------------------------------------------------------------------------
				; Name: printDialogue
				;
				; Prints dialogue.
				;
				; Preconditions: none.
				;
				; Postconditions: Value stored in EAX.
				;
				; Receives:
				;	[EBP+8]	= address of dialogue
				;
				; Returns: none.
				; ---------------------------------------------------------------------------------
 000000B0			printDialogue PROC
					; Procedure prep
 000000B0  55				push EBP
 000000B1  8B EC			mov EBP, ESP

					; Display dialogue
 000000B3  B8 00000009			mov EAX, DIALOGUE_DISPLAY
 000000B8  E8 00000000 E		call SetTextColor

 000000BD  E8 00000000 E		call CrLf
 000000C2  8B 55 08			mov EDX, [EBP+8]
 000000C5  E8 00000000 E		call WriteString
 000000CA  E8 00000000 E		call CrLf

 000000CF  B8 0000000F			mov EAX, DEFAULT_DISPLAY
 000000D4  E8 00000000 E		call SetTextColor

					; Procedure clean up
 000000D9  5D				pop EBP
 000000DA  C2 0004			ret 4
 000000DD			printDialogue ENDP

