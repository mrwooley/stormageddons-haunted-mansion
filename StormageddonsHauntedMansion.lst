				checkChoice MACRO lower_limit, upper_limit, choice
					LOCAL _InvalidChoice, _Leave
					cmp choice, lower_limit
					jl _InvalidChoice
					cmp choice, upper_limit
					jg _InvalidChoice
					jmp _Leave

				_InvalidChoice:
					mov choice, 0

				_Leave:
				ENDM

				getKeyPress MACRO
				LOCAL _LookForKey

				_LookForKey:
					mov EAX, 10
					call Delay
					call ReadKey
					jz _LookForKey

				ENDM

				getCursorPos MACRO
					push EAX
					invoke GetStdHandle, STD_OUTPUT_HANDLE
					mov hStdOut, EAX
				    invoke GetConsoleScreenBufferInfo, hStdOut, ADDR CursorPos
					pop EAX
				ENDM


